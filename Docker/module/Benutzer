        this.Nachname = Nachname;
        this.Fachsemester = Fachsemester;
        this.EMail = EMail;
        this.Passwort = Passwort;
        this.Bild = Bild;
        this.Einstellung = Einstellung;
        this.Kuerzel = Kuerzel;
    }


